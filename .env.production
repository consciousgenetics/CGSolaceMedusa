NODE_ENV=production
MEDUSA_ADMIN_ONBOARDING_TYPE=default
# Update these with your actual production domains
STORE_CORS=https://*.railway.app,https://*.up.railway.app,http://localhost:8000
ADMIN_CORS=https://*.railway.app,https://*.up.railway.app,http://localhost:7000
AUTH_CORS=https://*.railway.app,https://*.up.railway.app,http://localhost:7000
# Redis configuration
REDIS_URL=${REDIS_URL}
# Database configuration
DATABASE_URL=${DATABASE_URL}
DATABASE_TYPE=postgres
# Security
JWT_SECRET=${JWT_SECRET}
COOKIE_SECRET=${COOKIE_SECRET}
# Medusa settings
PORT=9000
# Disable workflows to fix deployment issues
MEDUSA_FF_WORKFLOWS=false
# Disable IPv6
NODE_OPTIONS=--dns-result-order=ipv4first 